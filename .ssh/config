Host github
 HostName github.com
 User git
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa


Host github-yanglab
 HostName github.com
 User git
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa_yanglab

Host olive
    XAuthLocation /opt/X11/bin/xauth
    ForwardX11 yes
